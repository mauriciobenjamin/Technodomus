extends ../../_layouts/productos

block append config
  //- Correct base url pathing for this page
  - var baseUrl = './../../'
  - var urlLinks = '/esterilizacion/lavado/'
  - var tituloPagina = "Technodomus | Soluciones en lavado"

block content
  +jumbo('Químicos para el lavado de instrumental médico')
  ol.breadcrumb
    li
      a(href="./../../") Inicio
    li
      a(href="./../") Esterilización
    li
      a(href="./") Lavado
    li.active
      a(href="#") Insumos
  
  .grid
    +panelprod('Insumos de alta calidad')
      .panel-body
        p En Technodomus sabemos que el equipo no es todo, requiere de personal adecuado y de insumos de la mejor calidad para funcionar correctamente, por ello le ofrecemos una variedad de insumos mejorar sus procesos de lavado y sacar el máximo provecho a los equipos e instrumental de su institución.
        h4
          a(href= "./detergente-enzimatico.html") Detergentes enzimáticos
        h4
          a(href= "./abrillantador.html") Abrillantador
        h4
          a(href= "./desinfectantes.html") Desinfectantes
        h4
          a(href= "./desincrustante.html") Desincrustante

    +panelsubp('Detergente enzimático','detergente-enzimatico.html')
      .panel-body
        a(href= "./detergente-enzimatico.html")
          +foto('Detergente-EmPower-Metrex.png', 'Detergente Enzimatico')    

    +panelsubp('Abrillantador','abrillantador.html')
      .panel-body
        a(href= "./abrillantador.html") 
          +foto('Lubricante-MetriLube-Metrex.jpg', 'Abrillantador')

    +panelsubp('Desinfectantes','desinfectantes.html')
      .panel-body
        a(href= "./desinfectantes.html")
          +foto('Desinfectante-CaviCide-Metrex.jpg', 'Desinfectante')

    +panelsubp('Desincrustante','desincrustante.html')
      .panel-body
        a(href= "./desincrustante.html")
          +foto('lavado-solucion-MetriShine-Metrex.png', 'Desincrustante')  
